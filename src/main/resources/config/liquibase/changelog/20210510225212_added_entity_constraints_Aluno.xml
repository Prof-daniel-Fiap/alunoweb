        <addForeignKeyConstraint baseColumnNames="turma_id"
                                 baseTableName="rel_aluno__turma"
                                 constraintName="fk_rel_aluno__turma__turma_id"
                                 referencedColumnNames="id"
                                 referencedTableName="turma"/>
    </changeSet>
</databaseChangeLog>
